   "source": [
    "import requests\n",
    "import base64\n",
    "url = \"http://127.0.0.1:8000/predict/\"\n",
    "with open(r\"./이미지 경로.jpg\", 'rb') as img:\n",
    "    base64_string = base64.b64encode(img.read())\n",
    "r = requests.post(url, data = {'image' : base64_string})\n",
    "r.text"
   ]
